
//Write a program in cpp to add 2 numbers.

#include <iostream>
using namespace std;

int main() {
    int num1, num2, sum;
    cout << "Enter first number: ";
    cin >> num1;
    cout << "Enter second number: ";
    cin >> num2;
    sum = num1 + num2;
    cout << "Sum: " << sum << endl;
    return 0;
}

