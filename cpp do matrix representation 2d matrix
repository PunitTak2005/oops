#include <iostream>
#include <vector>

using namespace std;

// Define a type alias for a 2D matrix
typedef vector<vector<int>> Matrix;

// Function to print a 2D matrix
void printMatrix(const Matrix& matrix) {
    for (const auto& row : matrix) {
        for (int elem : row) {
            cout << elem << " ";
        }
        cout << endl;
    }
}

int main() {
    // Create a 2D matrix
    Matrix matrix = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};

    // Print the matrix
    printMatrix(matrix);

    return 0;
}
